prefix=@CMAKE_INSTALL_PREFIX@
libdir=@CMAKE_INSTALL_PREFIX@/lib
includedir=@CMAKE_INSTALL_PREFIX@/include

Name: raftlib 
URL: http://raftlib.io
Description: RaftLib C++ Streaming/Data-flow Library
Version: 2020.6
Requires: shm affinity demangle
Conflicts: 
Libs:  -L${libdir} -lraft -lshm -laffinity -ldemangle -lpthread -lrt 
Libs.private: shm affinity demangle
Cflags:  -std=c++14 -DL1D_CACHE_LINE_SIZE=@L1D_LINE_SIZE@ -I${includedir}
